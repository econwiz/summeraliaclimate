. * helper program: load ster, dump e(b) to CSV
. capture program drop est_to_csv

. program define est_to_csv
  1.     syntax using/, SAVE(string)
  2. 
.     estimates use "`using'", clear
  3. 
.     matrix b = e(b)
  4.     local k = colsof(b)
  5.     local names : colnames b
  6. 
.     preserve
  7.         clear
  8.         set obs `k'
  9.         gen strL parm = ""
 10.         gen double beta = .
 11. 
.         local j = 1
 12.         foreach nm of local names {
 13.             replace parm = "`nm'"   in `j'
 14.             replace beta = b[1,`j'] in `j'
 15.             local ++j
 16.         }
 17. 
.         order parm beta
 18.         export delimited using "`save'", replace
 19.     restore
 20. end

. 
